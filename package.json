{
  "name": "cargonaut-ws",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ts-node -P ./server/server.tsconfig.json ./server/server.ts",
    "start-dev": "ng serve --proxy-config proxy.json",
    "serve-dev": "ts-node -P ./server/server.tsconfig.json ./server/server.ts",
    "build": "yarn build-lib && ng build demo --aot --prod",
    "build-dev": "yarn build-lib && ng build demo",
    "build-lib": "ng build",
    "test": "ng test --browsers=ChromeHeadless",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "verify": "yarn verify-dev && yarn verify-prod",
    "verify-dev": "yarn cache clean -y && yarn && yarn build && yarn build-dev && yarn test-headless && yarn lint",
    "verify-prod": "yarn cache clean -y && yarn heroku-postbuild",
    "verify-dep": "yarn cache clean -y && yarn && yarn verify",
    "compodoc-generate": "npx compodoc -p projects/cargo-domain/tsconfig.lib.json",
    "compodoc-start": "compodoc -p projects/cargo-domain/tsconfig.lib.json -s",
    "heroku-postbuild": "yarn build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/cli": "~8.3.20",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ngrx/data": "^8.6.0",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/entity": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "express": "^4.16.2",
    "hammerjs": "^2.0.8",
    "ng-gist": "^1.0.2",
    "path": "^0.12.7",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "typescript": "~3.5.3",
    "uuid": "^3.3.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular-devkit/build-ng-packagr": "~0.803.20",
    "@angular/language-service": "~8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/express": "^4.0.39",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "enhanced-resolve": "3.3.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "engines": {
    "node": "12.3.1",
    "npm": "6.9.0"
  }
}
